    "@testing-library/dom": "^6.11.0",
    "@testing-library/jest-dom": "^4.2.4",
    "acorn-optional-chaining": "^0.0.3",
    "babel-jest": "^24.8.0",
    "jest": "^24.8.0",
    "jsdom-simulant": "^1.1.2",
